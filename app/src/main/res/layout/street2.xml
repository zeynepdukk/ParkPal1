<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/street2"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <Button
            android:id="@+id/parking2_backbutton"
            android:layout_width="100dp"
            android:layout_height="80dp"
            android:background="@android:color/transparent">
        </Button>
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <Button
                android:id="@+id/parking2_a1"
                android:layout_width="100dp"
                android:layout_height="140dp"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="30dp">
            </Button>
            <ImageView
                android:id="@+id/a1_imageView"
                android:layout_width="130dp"
                android:layout_height="120dp"
                android:layout_weight="1"
                android:layout_marginTop="30dp"
                android:layout_marginLeft="-120dp"
                android:background="@drawable/cartop"
                android:visibility="visible" />

            <Button
                android:id="@+id/parking2_a2"
                android:layout_width="100dp"
                android:layout_height="140dp"
                android:layout_marginLeft="-110dp"
                android:layout_marginTop="160dp"
                android:width="100dp"
                android:height="100dp">
            </Button>
            <ImageView
                android:id="@+id/a2_imageView"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:layout_marginStart="-112dp"
                android:layout_marginTop="160dp"
                android:layout_weight="1"
                android:background="@drawable/cartop"
                android:visibility="visible" />
            <Button
                android:id="@+id/parking2_a3"
                android:layout_width="100dp"
                android:layout_height="140dp"
                android:layout_marginLeft="-110dp"
                android:layout_marginTop="300dp"
                android:width="100dp"
                android:height="100dp">
            </Button>
            <ImageView
                android:id="@+id/a3_imageView"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:layout_marginStart="-112dp"
                android:layout_marginTop="300dp"
                android:layout_weight="1"
                android:background="@drawable/cartop"
                android:visibility="visible" />
            <Button
                android:id="@+id/parking2_a4"
                android:layout_width="100dp"
                android:layout_height="140dp"
                android:layout_marginLeft="-108dp"
                android:layout_marginTop="430dp"
                android:width="100dp"
                android:height="100dp">
            </Button>
            <ImageView
                android:id="@+id/a4_imageView"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:layout_marginStart="-112dp"
                android:layout_marginTop="430dp"
                android:layout_weight="1"
                android:background="@drawable/cartop"
                android:visibility="visible" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="90dp"
            android:orientation="horizontal">
            <Button
                android:id="@+id/parking2_b1"
                android:layout_width="100dp"
                android:layout_height="140dp"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="30dp">
            </Button>
            <ImageView
                android:id="@+id/b1_imageView"
                android:layout_width="130dp"
                android:layout_height="120dp"
                android:layout_weight="1"
                android:layout_marginTop="30dp"
                android:layout_marginLeft="-120dp"
                android:background="@drawable/cartop"
                android:visibility="visible" />

            <Button
                android:id="@+id/parking2_b2"
                android:layout_width="100dp"
                android:layout_height="140dp"
                android:layout_marginLeft="-110dp"
                android:layout_marginTop="160dp"
                android:width="100dp"
                android:height="100dp">
            </Button>
            <ImageView
                android:id="@+id/b2_imageView"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:layout_marginStart="-112dp"
                android:layout_marginTop="160dp"
                android:layout_weight="1"
                android:background="@drawable/cartop"
                android:visibility="visible" />
            <Button
                android:id="@+id/parking2_b3"
                android:layout_width="100dp"
                android:layout_height="140dp"
                android:layout_marginLeft="-110dp"
                android:layout_marginTop="300dp"
                android:width="100dp"
                android:height="100dp">
            </Button>
            <ImageView
                android:id="@+id/b3_imageView"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:layout_marginStart="-112dp"
                android:layout_marginTop="300dp"
                android:layout_weight="1"
                android:background="@drawable/cartop"
                android:visibility="visible" />
            <Button
                android:id="@+id/parking2_b4"
                android:layout_width="100dp"
                android:layout_height="140dp"
                android:layout_marginLeft="-108dp"
                android:layout_marginTop="430dp"
                android:width="100dp"
                android:height="100dp">
            </Button>
            <ImageView
                android:id="@+id/b4_imageView"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:layout_marginStart="-112dp"
                android:layout_marginTop="430dp"
                android:layout_weight="1"
                android:background="@drawable/cartop"
                android:visibility="visible" />
        </LinearLayout>
    </LinearLayout>


</LinearLayout>